{
  "name": "easy-validation-js",
  "version": "1.3.2",
  "description": "javascript validation library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "npx nodemon",
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "build2": "tsup src/index.ts --cjsInterop",
    "start:cjs": "node dist/cjs/index.js",
    "start:esm": "node dist/esm/index.js",
    "script": "node script.js"
  },
  "directories": {
    "dist": "dist`"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeveloperRejaul/easy-validation-js.git"
  },
  "author": "Rejaul Karim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeveloperRejaul/easy-validation-js/issues"
  },
  "keywords": [
    "javascript validation",
    "javascript",
    "easy-validation",
    "validation",
    "easy",
    "easy-validation-js",
    "js-validation"
  ],
  "homepage": "https://github.com/DeveloperRejaul/easy-validation-js#readme",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/node": "^20.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "esbuild": "0.23.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.2.3",
    "typescript": "^5.1.6"
  }
}
